		УстановитьСубконто(Проводка.СчетКт,Проводка.СубконтоКт,1,ТекСтрокаГотоваяПродукция.Цех);
		УстановитьСубконто(Проводка.СчетКт,Проводка.СубконтоКт,2,Справочники.СтатьиЗатрат.Выпуск);
		УстановитьСубконто(Проводка.СчетКт,Проводка.СубконтоКт,3,ТекСтрокаГотоваяПродукция.Номенклатура);
		
		Проводка.Сумма = ТекСтрокаГотоваяПродукция.Сумма;
		Проводка.КоличествоДт = ТекСтрокаГотоваяПродукция.Количество;
		Проводка.КоличествоКт = ТекСтрокаГотоваяПродукция.Количество;
		
	КонецЦикла;
	
КонецПроцедуры
 

