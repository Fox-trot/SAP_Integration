	Для Каждого ТекСтрокаГотоваяПродукция Из ГотоваяПродукция Цикл
		// регистр Хозрасчетный 
		Проводка = Движения.Хозрасчетный.Добавить();
		Проводка.Организация = Организация;
		Проводка.Период = Дата;
		
		Проводка.СчетДт = ТекСтрокаГотоваяПродукция.СчетУчета;
		УстановитьСубконто(Проводка.СчетДт,Проводка.СубконтоДт,1,ТекСтрокаГотоваяПродукция.Номенклатура);
