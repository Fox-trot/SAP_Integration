		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ХозрасчетныйОстатки.Субконто1 КАК Номенклатура,
	ХозрасчетныйОстатки.Субконто2 КАК Склад,
	ХозрасчетныйОстатки.Счет,
	ХозрасчетныйОстатки.КоличествоОстатокДт КАК КоличествоДляХраненияОстатков,
	"Остаток на начало" КАК Секция,
	1 КАК ДляСортировки,
	NULL КАК Регистратор,
	ХозрасчетныйОстатки.КоличествоОстатокДт * ХозрасчетныйОстатки.Субконто1.ЕдиницаХраненияОстатков.Коэффициент / ХозрасчетныйОстатки.Субконто1.ЕдиницаДляОтчетов.Коэффициент КАК КоличествоДляОтчетов,
	ХозрасчетныйОстатки.КоличествоОстатокДт * ХозрасчетныйОстатки.Субконто1.ЕдиницаХраненияОстатков.Коэффициент КАК Количество
ИЗ
	РегистрБухгалтерии.Хозрасчетный.Остатки(
			&amp;НачалоПериода {(&amp;НачалоПериода)},
			Счет В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.а2800))
				ИЛИ Счет В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.а2900)),
			,
			Организация = &amp;Организация
				И ВЫБОР
					КОГДА &amp;Номенклатура = ЗНАЧЕНИЕ(Справочник.НоменклатураПродаж.ПустаяСсылка)
						ТОГДА ИСТИНА
					ИНАЧЕ Субконто1 = &amp;Номенклатура
				КОНЕЦ
				И ВЫБОР
					КОГДА &amp;Склад = ЗНАЧЕНИЕ(Справочник.Склады.ПустаяСсылка)
						ТОГДА ИСТИНА
					ИНАЧЕ Субконто2 = &amp;Склад
				КОНЕЦ) КАК ХозрасчетныйОстатки

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ХозрасчетныйОбороты.Субконто1,
	ХозрасчетныйОбороты.Субконто2,
	ХозрасчетныйОбороты.Счет,
	ХозрасчетныйОбороты.КоличествоОборотДт,
	"Приход",
	2,
	ХозрасчетныйОбороты.Регистратор,
	ХозрасчетныйОбороты.КоличествоОборотДт * ХозрасчетныйОбороты.Субконто1.ЕдиницаХраненияОстатков.Коэффициент / ХозрасчетныйОбороты.Субконто1.ЕдиницаДляОтчетов.Коэффициент,
	ХозрасчетныйОбороты.КоличествоОборотДт * ХозрасчетныйОбороты.Субконто1.ЕдиницаХраненияОстатков.Коэффициент
ИЗ
	РегистрБухгалтерии.Хозрасчетный.Обороты(
			&amp;НачалоПериода {(&amp;НачалоПериода)},
			&amp;КонецПериода {(&amp;КонецПериода)},
			Регистратор,
			Счет В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.а2800))
				ИЛИ Счет В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.а2900)),
			,
			Организация = &amp;Организация
				И ВЫБОР
					КОГДА &amp;Номенклатура = ЗНАЧЕНИЕ(Справочник.НоменклатураПродаж.ПустаяСсылка)
						ТОГДА ИСТИНА
					ИНАЧЕ Субконто1 = &amp;Номенклатура
				КОНЕЦ
				И ВЫБОР
					КОГДА &amp;Склад = ЗНАЧЕНИЕ(Справочник.Склады.ПустаяСсылка)
						ТОГДА ИСТИНА
					ИНАЧЕ Субконто2 = &amp;Склад
				КОНЕЦ,
			,
			) КАК ХозрасчетныйОбороты

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ХозрасчетныйОбороты.Субконто1,
	ХозрасчетныйОбороты.Субконто2,
	ХозрасчетныйОбороты.Счет,
	ХозрасчетныйОбороты.КоличествоОборотКт,
	"Расход",
	3,
	ХозрасчетныйОбороты.Регистратор,
	ХозрасчетныйОбороты.КоличествоОборотКт * ХозрасчетныйОбороты.Субконто1.ЕдиницаХраненияОстатков.Коэффициент / ХозрасчетныйОбороты.Субконто1.ЕдиницаДляОтчетов.Коэффициент,
	ХозрасчетныйОбороты.КоличествоОборотКт * ХозрасчетныйОбороты.Субконто1.ЕдиницаХраненияОстатков.Коэффициент
ИЗ
	РегистрБухгалтерии.Хозрасчетный.Обороты(
			&amp;НачалоПериода {(&amp;НачалоПериода)},
			&amp;КонецПериода {(&amp;КонецПериода)},
			Регистратор,
			Счет В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.а2800))
				ИЛИ Счет В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.а2900)),
			,
			Организация = &amp;Организация
				И ВЫБОР
					КОГДА &amp;Номенклатура = ЗНАЧЕНИЕ(Справочник.НоменклатураПродаж.ПустаяСсылка)
						ТОГДА ИСТИНА
					ИНАЧЕ Субконто1 = &amp;Номенклатура
				КОНЕЦ
				И ВЫБОР
					КОГДА &amp;Склад = ЗНАЧЕНИЕ(Справочник.Склады.ПустаяСсылка)
						ТОГДА ИСТИНА
					ИНАЧЕ Субконто2 = &amp;Склад
				КОНЕЦ,
			,
			) КАК ХозрасчетныйОбороты

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ХозрасчетныйОстатки.Субконто1,
	ХозрасчетныйОстатки.Субконто2,
	ХозрасчетныйОстатки.Счет,
	ХозрасчетныйОстатки.КоличествоОстатокДт,
	"Остаток на конец",
	4,
	NULL,
	ХозрасчетныйОстатки.КоличествоОстатокДт * ХозрасчетныйОстатки.Субконто1.ЕдиницаХраненияОстатков.Коэффициент / ХозрасчетныйОстатки.Субконто1.ЕдиницаДляОтчетов.Коэффициент,
	ХозрасчетныйОстатки.КоличествоОстатокДт * ХозрасчетныйОстатки.Субконто1.ЕдиницаХраненияОстатков.Коэффициент
ИЗ
	РегистрБухгалтерии.Хозрасчетный.Остатки(
			ДОБАВИТЬКДАТЕ(&amp;КонецПериода, СЕКУНДА, 1) {(&amp;КонецПериода)},
			Счет В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.а2800))
				ИЛИ Счет В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.а2900)),
			,
			Организация = &amp;Организация
				И ВЫБОР
					КОГДА &amp;Номенклатура = ЗНАЧЕНИЕ(Справочник.НоменклатураПродаж.ПустаяСсылка)
						ТОГДА ИСТИНА
					ИНАЧЕ Субконто1 = &amp;Номенклатура
				КОНЕЦ
				И ВЫБОР
					КОГДА &amp;Склад = ЗНАЧЕНИЕ(Справочник.Склады.ПустаяСсылка)
						ТОГДА ИСТИНА
					ИНАЧЕ Субконто2 = &amp;Склад
				КОНЕЦ) КАК ХозрасчетныйОстатки</query>
