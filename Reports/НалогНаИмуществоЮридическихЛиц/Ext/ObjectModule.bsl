
Процедура ВывестиРасшифровкуДляПриложения3(Выборка, Период)

	//Состояние("Расшифровка - не введенные в срок основные средства...");
	
	РасшТаб = Новый ТабличныйДокумент;
	РасшМакет = ПолучитьМакет("РасшифровкаДляПриложения3");
	РасшОбластьШапка = РасшМакет.ПолучитьОбласть("Шапка");
	РасшОбластьСтрока = РасшМакет.ПолучитьОбласть("Строка");
	РасшОбластьПодвал = РасшМакет.ПолучитьОбласть("Подвал");
	
	РасшТаб.Вывести(РасшОбластьШапка);     	
	
	Стоимость = 0;
	
	нпп = 0;
	Пока Выборка.Следующий() Цикл
		нпп = нпп + 1;
		//Состояние("Расшифровка не введенные в срок основные средства "+Строка(нпп));
		РасшОбластьСтрока.Параметры.Заполнить(Выборка);
		РасшОбластьСтрока.Параметры.нпп = нпп;
		РасшТаб.Вывести(РасшОбластьСтрока);
        Стоимость = Стоимость + Выборка.СуммаОстаток;
	КонецЦикла; 
	
	РасшОбластьПодвал.Параметры.Стоимость = Стоимость;
	РасшТаб.Вывести(РасшОбластьПодвал);
	РасшТаб.Показать("Расшифровка - не введенные в срок основные средства ("+Формат(Период, "ДФ='MMMM yyyy'")+")");
	
КонецПроцедуры	
	
Функция Приложение3_Получить(НачалоПериода, КонецПериода, РасшифровкаРазрешена = Истина) Экспорт
	
	Перем Сумма;
	
	//Состояние("Не введенные в срок основные средства...");
	
	Сумма = 0;
	
	ТекПериод = КонецМесяца(НачалоПериода);
	
	Пока Не ТекПериод = КонецМесяца(КонецПериода) Цикл
		
		Запрос = Новый Запрос;
		Запрос.Текст = "ВЫБРАТЬ
		               |	Номенклатура.Ссылка КАК ОС,
		               |	Номенклатура.Код КАК ИН,
		               |	Номенклатура.ДатаПоступленияОсновногоСредства,
		               |	Номенклатура.ДатаВводаВЭксплуатацию,
		               |	СУММА(ЕСТЬNULL(ХозрасчетныйОстатки.СуммаОстаток, 0)) КАК СуммаОстаток
		               |ИЗ
		               |	Справочник.Номенклатура КАК Номенклатура
		               |		ЛЕВОЕ СОЕДИНЕНИЕ РегистрБухгалтерии.Хозрасчетный.Остатки(
		               |				&Период,
		               |				Счет В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.А0800)),
		               |				,
		               |				ВЫБОР
		               |					КОГДА &Организация = ЗНАЧЕНИЕ(Справочник.Организации.ПустаяСсылка)
		               |						ТОГДА ИСТИНА
		               |					ИНАЧЕ Организация = &Организация
		               |				КОНЕЦ) КАК ХозрасчетныйОстатки
		               |		ПО Номенклатура.Ссылка = ХозрасчетныйОстатки.Субконто1
		               |ГДЕ
		               |	Номенклатура.ВидНоменклатуры = ЗНАЧЕНИЕ(Перечисление.ВидыНоменклатуры.ПриобретенныеОсновныеСредства)
		               |	И ХозрасчетныйОстатки.СуммаОстаток > 0
		               |	И Номенклатура.ДатаВводаВЭксплуатацию <> ДАТАВРЕМЯ(1, 1, 1)
		               |	И Номенклатура.ДатаВводаВЭксплуатацию < КОНЕЦПЕРИОДА(&Период, МЕСЯЦ)
		               |
		               |СГРУППИРОВАТЬ ПО
		               |	Номенклатура.Ссылка,
		               |	Номенклатура.Код,
		               |	Номенклатура.ДатаПоступленияОсновногоСредства,
		               |	Номенклатура.ДатаВводаВЭксплуатацию
		               |ИТОГИ
		               |	СУММА(СуммаОстаток)
		               |ПО
		               |	ОБЩИЕ";
		
		Запрос.УстановитьПараметр("Период", ТекПериод);
		Запрос.УстановитьПараметр("Организация", Организация); 
		
		Результат = Запрос.Выполнить();
		ВыборкаИтог = Результат.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
		
		Если ВыборкаИтог.Следующий() Тогда
			Сумма = Сумма + ?(ВыборкаИтог.СуммаОстаток=Null,0,ВыборкаИтог.СуммаОстаток);
			Если РасшифровыватьПриложение3и4 И РасшифровкаРазрешена Тогда
				ВывестиРасшифровкуДляПриложения3(ВыборкаИтог.Выбрать(), ТекПериод);		
			КонецЕсли; 
		КонецЕсли;         		
		
		ТекПериод = ДобавитьМесяц(ТекПериод, 1);
		ТекПериод = КонецМесяца(ТекПериод);
		
	КонецЦикла; 
	
	Возврат Сумма/12;
	
КонецФункции

Процедура ВывестиРасшифровкуДляЛьготныхОСПриложения4(Выборка, Период)

	//Состояние("Расшифровка - льготные основные средства...");
	
	СуммаЛьготныхОС = 0;
	
	РасшТаб = Новый ТабличныйДокумент;
	РасшМакет = ПолучитьМакет("РасшифровкаДляПриложения4");
	РасшОбластьШапка = РасшМакет.ПолучитьОбласть("Шапка");
	РасшОбластьСтрока = РасшМакет.ПолучитьОбласть("Строка");
	РасшОбластьПодвал = РасшМакет.ПолучитьОбласть("Подвал");
	
	РасшТаб.Вывести(РасшОбластьШапка);     	
	
	нпп = 0;
	Пока Выборка.Следующий() Цикл
		нпп = нпп + 1;
		РасшОбластьСтрока.Параметры.Заполнить(Выборка);
		РасшОбластьСтрока.Параметры.Месяц = Формат(Период, "ДФ=MMMM.yyyy");
		РасшОбластьСтрока.Параметры.нпп = нпп;
		РасшТаб.Вывести(РасшОбластьСтрока);
		СуммаЛьготныхОС = СуммаЛьготныхОС + ?(Выборка.ОстаточнаяСтоимость=Null,0,Выборка.ОстаточнаяСтоимость);
	КонецЦикла;
	
	РасшОбластьПодвал.Параметры.ОстаточнаяСтоимость = СуммаЛьготныхОС;
	РасшТаб.Вывести(РасшОбластьПодвал);
	РасшТаб.Показать("Расшифровка - льготные основные средства ("+Формат(Период, "ДФ='MMMM yyyy'")+")");
	
КонецПроцедуры

