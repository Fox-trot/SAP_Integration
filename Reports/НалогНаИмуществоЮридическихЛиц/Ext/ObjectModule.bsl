		ВыборкаИтог = Результат.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
		
		Если ВыборкаИтог.Следующий() Тогда		
			СуммаЛизинговыхОС = СуммаЛизинговыхОС + ?(ВыборкаИтог.ОстаточнаяСтоимость=Null,0,ВыборкаИтог.ОстаточнаяСтоимость);
			Если РасшифровыватьПриложение3и4 И РасшифровкаРазрешена Тогда
				ВывестиРасшифровкуДляЛизинговыхОСПриложения4(ВыборкаИтог.Выбрать(), ТекПериод);
			КонецЕсли; 
		КонецЕсли;	
		
		ТекПериод = ДобавитьМесяц(ТекПериод, 1);
		ТекПериод = КонецМесяца(ТекПериод);
		
	КонецЦикла; 
	
	а02011 = СуммаЛизинговыхОС/12;
	
	
	а020 = а0207 + а02011;
	а010 = а020;
	
	Строки.Вставить("а010", а010);
