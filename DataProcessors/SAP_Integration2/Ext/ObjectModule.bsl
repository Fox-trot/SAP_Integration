Процедура УслугиСтороннихСоздатьНаСервере(ТЧ=Неопределено) Экспорт
	Если ТЧ = Неопределено Тогда
		ТЧ = ZFI_011_S_DATA;
	//Иначе
	//	//Настроить(ЭтотОбъект);
	КонецЕсли;
	ТЧ.Сортировать("DOCNO,POSTITEMFORLEDGER");
	
	мДоки	= Новый Массив;
	Для Каждого ТекСтрока Из ТЧ Цикл
		Если мДоки.Найти(ТекСтрока.DOCNO) = Неопределено Тогда
			Строки = ТЧ.НайтиСтроки(Новый Структура("DOCNO", ТекСтрока.DOCNO));
			Если DOCTYP_Банк(ТекСтрока.DOCTYP) Тогда
				ВыпискаБанкаСоздать(ТекСтрока, Строки);
				
			ИначеЕсли СтрСравнить(ТекСтрока.DOCTYP, "KR") = 0 Тогда
				СчетФактураВыданныйСоздать(ТекСтрока, Строки);
			ИначеЕсли СтрСравнить(ТекСтрока.DOCTYP, "AA") = 0 Тогда
				ПринятиеКУчетуОССоздать(ТекСтрока, Строки);
			ИначеЕсли СтрСравнить(ТекСтрока.DOCTYP, "AC") = 0 Тогда
				//МодернизацияОССоздать(ТекСтрока, Строки);
			ИначеЕсли СтрСравнить(ТекСтрока.DOCTYP, "AF") = 0 Тогда
				НачислениеАмортизацииОССоздать(ТекСтрока, Строки);
			ИначеЕсли СтрСравнить(ТекСтрока.DOCTYP, "AS") = 0 Тогда
				//ПередачаОССоздать(ТекСтрока, Строки);
				
			ИначеЕсли СтрСравнить(ТекСтрока.DOCTYP, "DR") = 0 Тогда
				УслугиСтороннихОрганизацийСоздать(ТекСтрока, Строки);
			КонецЕсли;
			
			мДоки.Добавить(ТекСтрока.DOCNO);
		КонецЕсли;
	КонецЦикла;
КонецПроцедуры

