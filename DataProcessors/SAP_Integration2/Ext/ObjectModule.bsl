Процедура ДокЗаписать(Док, Ошибки=Неопределено)
	Если ЕстьРеквизитДокумента("Ответственный", Док.Метаданные()) Тогда
		Док.Ответственный	= Ответственный;
	КонецЕсли;
	Если ТипЗнч(Ошибки) = Тип("Массив") Тогда
		Док.Комментарий		= СтрСоединить(Ошибки, ",");
	КонецЕсли;
	
	Если НЕ ПривилегированныйРежим() Тогда
		УстановитьПривилегированныйРежим(Истина);
	КонецЕсли;
	Если Док.ПроверитьЗаполнение() Тогда
		Попытка
			Если Док.Ссылка.Пустая() Тогда
				Док.Записать(РежимЗаписиДокумента.Запись);
			КонецЕсли;
			Док.Записать(РежимЗаписиДокумента.Проведение, РежимПроведенияДокумента.Неоперативный);
		Исключение
			//Если ТипЗнч(Ошибки) = Тип("Массив") Тогда
			//	Ошибки.Добавить(ОписаниеОшибки());
				//Док.Комментарий	= СтрСоединить(Ошибки, ",");
			//test = "";
		КонецПопытки;
	КонецЕсли;
	Если Док.Модифицированность() Тогда
		Попытка
			Док.Записать(РежимЗаписиДокумента.Запись);
		Исключение КонецПопытки;
	КонецЕсли;
КонецПроцедуры

Функция УслугиСтороннихОрганизацийПроведен(Параметры)
