Процедура ПринятиеКУчетуОССоздать(Шапка, Детали)
	Ошибки		= Новый Массив;
	Ссылка		= ПринятиеКУчетуОСНайти(Шапка);
	Если ЗначениеЗаполнено(Ссылка) Тогда
		Док = Ссылка.ПолучитьОбъект();
		Если Док.ПометкаУдаления Тогда
			Док.УстановитьПометкуУдаления(Ложь);
		КонецЕсли;
		Док.ОсновныеСредства.Очистить();
		Док.Оплата.Очистить();
		Док.СтроительныеЗатраты.Очистить();

	Иначе
		Док = Документы.ПринятиеКУчетуОС.СоздатьДокумент();
		Док.Дата			= Шапка.DOCDATE;
		Док.Номер			= Шапка.DOCNO;
		Док.Организация		= Организация;
	КонецЕсли;
	Док.ОтражатьПоБУ	= Истина;
	ПринятиеКУчетуОС_Дефолт(Док);

	Для Каждого Деталь Из Детали Цикл
		Если Деталь.AMOUNT_COMPANYCODECURR > 0 Тогда
			ТекСтрока = Док.ОсновныеСредства.Добавить();
			ТекСтрока.ОсновноеСредство	= ОсновноеСредствоНайти(Деталь.ASSET_NO);
			ТекСтрока.ИнвентарныйНомер	= Деталь.ASSET_NO;
			
			Док.СтоимостьБУ				= Деталь.AMOUNT_COMPANYCODECURR;
		КонецЕсли;
	КонецЦикла;
	
	ДокЗаписать(Док, Ошибки);
КонецПроцедуры

